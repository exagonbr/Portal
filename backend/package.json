{
  "name": "portal-educacional",
  "version": "1.0.0",
  "private": true,
  "uniqueName": "portal-educacional",
  "description": "Backend API para o Portal Sabercon - Sistema de GestÃ£o Educacional com Redis Sessions e Dashboard Analytics",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run migrate:latest && nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typeorm:cli": "npx typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm:cli migration:generate",
    "migration:run": "npm run typeorm:cli migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm:cli migration:revert -d src/config/typeorm.config.ts",
    "setup": "node src/scripts/setup.ts",
    "create-env": "node scripts/create-env.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:aws": "node scripts/run-aws-migrations.js",
    "migrate:mysql": "ts-node scripts/migrate-mysql-simple.ts",
    "migrate:mysql:complete": "ts-node scripts/run-complete-migration.ts",
    "migrate:mysql:verify": "ts-node scripts/verify-migration.ts",
    "migrate:mysql:only": "ts-node scripts/complete-mysql-migrator.ts",
    "seed": "knex seed:run",
    "seed:roles": "ts-node scripts/run-seeds.ts",
    "seed:fresh": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "db:setup": "npm run migrate && npm run seed:roles",
    "roles:update": "npm run seed:fresh",
    "database:fresh": "ts-node scripts/database-fresh.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "queue:dev": "nodemon src/queues/worker.ts",
    "migrate:latest": "ts-node ./node_modules/knex/bin/cli.js migrate:latest",
    "migrate:make": "knex migrate:make",
    "seed:run": "knex seed:run",
    "seed:make": "knex seed:make",
    "assign-teacher-role": "node -r ts-node/register src/scripts/assignTeacherRole.ts",
    "test:cors": "node ../scripts/test-cors.js",
    "test:cors:dev": "set BACKEND_URL=https://portal.sabercon.com.br/api&& set FRONTEND_URL=https://portal.sabercon.com.br&& node ../scripts/test-cors.js",
    "test:cors:prod": "set BACKEND_URL=https://portal.sabercon.com.br&& set FRONTEND_URL=https://portal.sabercon.com.br&& node ../scripts/test-cors.js"
  },
  "keywords": [
    "education",
    "portal",
    "api",
    "nodejs",
    "typescript",
    "postgresql",
    "redis",
    "sessions",
    "jwt",
    "dashboard",
    "analytics",
    "swagger"
  ],
  "author": "Portal Sabercon Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.820.0",
    "@aws-sdk/s3-request-presigner": "^3.820.0",
    "@types/bcrypt": "^5.0.2",
    "@types/ioredis": "^4.28.10",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/web-push": "^3.6.4",
    "@xmpp/jid": "^0.13.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "bullmq": "^4.15.4",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.1",
    "nodemailer": "^6.9.7",
    "objection": "^3.1.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.2",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.24",
    "web-push": "^3.6.7",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
