{
  "name": "portal-sabercon-backend",
  "version": "2.0.0",
  "description": "Backend API para o Portal Sabercon - Sistema de GestÃ£o Educacional com Redis Sessions e Dashboard Analytics",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typeorm:cli": "npx typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm:cli migration:generate",
    "migration:run": "npm run typeorm:cli migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm:cli migration:revert -d src/config/typeorm.config.ts",
    "setup": "node src/scripts/setup.ts",
    "create-env": "node scripts/create-env.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:aws": "node scripts/run-aws-migrations.js",
    "migrate:dump": "ts-node src/scripts/run-complete-migration.ts",
    "migrate:dump:dry": "ts-node src/scripts/run-complete-migration.ts -- --dry-run",
    "migrate:dump:test": "ts-node src/scripts/test-dump-parser.ts",
    "migrate:legados": "ts-node scripts/migrar-dados-legados.ts",
    "replicate": "ts-node scripts/mysql-replication.ts",
    "replicate:test": "ts-node scripts/mysql-replication.ts test",
    "replicate:sync": "ts-node scripts/mysql-replication.ts sync",
    "replicate:start": "ts-node scripts/mysql-replication.ts start",
    "replicate:status": "ts-node scripts/mysql-replication.ts status",
    "replicate:clean": "find migrations -name '*_from_mysql.ts' -delete && find seeds -name '*_from_mysql.ts' -delete",
    "seed": "knex seed:run",
    "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "queue:dev": "nodemon src/queues/worker.ts",
    "clean:migrations": "ts-node scripts/clean-legacy-migrations.ts",
    "add:missing:columns": "ts-node scripts/add-missing-columns.ts",
    "migrate:fresh": "npm run clean:migrations && ts-node scripts/reset-migrations.ts && knex migrate:latest && npm run add:missing:columns && knex seed:run --specific=001_default_roles_and_permissions.ts"
  },
  "keywords": [
    "education",
    "portal",
    "api",
    "nodejs",
    "typescript",
    "postgresql",
    "redis",
    "sessions",
    "jwt",
    "dashboard",
    "analytics",
    "swagger"
  ],
  "author": "Portal Sabercon Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.820.0",
    "@aws-sdk/s3-request-presigner": "^3.820.0",
    "@types/bcrypt": "^5.0.2",
    "@types/ioredis": "^4.28.10",
    "@types/web-push": "^3.6.4",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "bullmq": "^4.15.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "objection": "^3.1.5",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.24",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
